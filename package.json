{
  "name": "angular-skeleton",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build @skt/core && ng build @skt/webapp",
    "build:nossr": "ng build @skt/core && ng build --configuration production,noSsr @skt/webapp",
    "core:storybook": "ng run @skt/core:storybook",
    "cy:run:webapp": "cypress run --project ./projects/webapp-e2e",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:code": "eslint \"./projects/**/*.ts\" \"./projects/**/*.html\"",
    "lint:code:fix": "eslint \"./projects/**/*.ts\" \"./projects/**/*.html\" --fix",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint:styles:fix": "stylelint --fix \"**/*.scss\"",
    "lighthouse": "lighthouse --output-path=lighthouse-report.html http://localhost:4200",
    "ng": "ng",
    "serve:ssr:webapp": "node dist/webapp/server/server.mjs",
    "start:webapp": "ng serve --project @skt/webapp",
    "test": "ng test --browsers ChromeHeadless",
    "tools:check-format": "zx tools/check-format.mjs",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "^19.2.3",
    "@angular/common": "^19.2.3",
    "@angular/compiler": "^19.2.3",
    "@angular/core": "^19.2.3",
    "@angular/forms": "^19.2.3",
    "@angular/platform-browser": "^19.2.3",
    "@angular/platform-browser-dynamic": "^19.2.3",
    "@angular/platform-server": "^19.2.3",
    "@angular/router": "^19.2.3",
    "@angular/ssr": "^19.2.4",
    "@tailwindcss/postcss": "^4.0.15",
    "@tanstack/angular-query-experimental": "^5.69.1",
    "express": "^4.21.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.3",
    "@chromatic-com/storybook": "^2.0.2",
    "@eslint/js": "^9.9.1",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-onboarding": "^8.4.4",
    "@storybook/angular": "^8.4.4",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@types/express": "^5.0.0",
    "@types/jasmine": "^5.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@web/test-runner": "^0.20.0",
    "angular-eslint": "^18.4.1",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "cypress": "^13.15.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-storybook": "^0.9.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonlint": "^1.6.3",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lighthouse": "^12.2.2",
    "ng-packagr": "^19.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.4.4",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "^4.0.15",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.15.0",
    "yaml": "^2.6.1",
    "zx": "^8.2.2"
  }
}
